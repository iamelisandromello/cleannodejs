    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "bcrypt": "^4.0.1",
